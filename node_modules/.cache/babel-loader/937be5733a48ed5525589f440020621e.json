e.lider;\r\n    },\r\n  },\r\n  actions: {\r\n    //Las acciones llaman a las mutaciones. A su vez las acciones pueden hacer llamadas a API's\r\n    async agregarProyectoAction(context) {\r\n      context.commit(\"agregandoProyecto\");\r\n    },\r\n    async editarProyectoAction(context) {\r\n      context.commit(\"editandoProyecto\");\r\n    },\r\n    async eliminarProyectoAction(context) {\r\n      context.commit(\"eliminandoProyecto\");\r\n    },\r\n    async agregarCategoriaAction(context) {\r\n      context.commit(\"agregandoCategoria\");\r\n    },\r\n    async editarCategoriaAction(context) {\r\n      context.commit(\"editandoCategoria\");\r\n    },\r\n    async eliminarCategoriaAction(context) {\r\n      context.commit(\"eliminandoCategoria\");\r\n    },\r\n    async agregarLiderAction(context) {\r\n      context.commit(\"agregandoLider\");\r\n    },\r\n    async editarLiderAction(context) {\r\n      context.commit(\"editandoLider\");\r\n    },\r\n    async eliminaLiderAction(context) {\r\n      context.commit(\"eliminandoLider\");\r\n    },\r\n  },\r\n  getters: {\r\n    getProyectos(state) {\r\n      return state.proyectos;\r\n    },\r\n    getCategorias(state) {\r\n      return state.categorias;\r\n    },\r\n    getLideres(state) {\r\n      return state.lideres;\r\n    },\r\n    getProyecto(state) {\r\n      return state.proyectos.find((p) => p.id == state.idProyectoBuscada);\r\n    },\r\n    getCategoria(state) {\r\n      return state.categorias.find((c) => c.id == state.idCategoriaBuscada);\r\n    },\r\n    getLider(state) {\r\n      return state.lideres.find((l) => l.id == state.idLiderBuscada);\r\n    },\r\n  },\r\n});\r\n"]}]}