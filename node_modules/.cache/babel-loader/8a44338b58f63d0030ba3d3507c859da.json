MAAA,KAAA,EAAA;AACA,QAAA,SAAA,EAAA,mBAAA,KAAA;AAAA,iBAAA,CAAA,CAAA,KAAA,IAAA,mBAAA;AAAA,SADA;AAEA,QAAA,QAAA,EAAA,kBAAA,KAAA;AAAA,iBAAA,KAAA,CAAA,MAAA,IAAA,EAAA,IAAA,sBAAA;AAAA;AAFA;AApBA,KAAA;AAyBA,GA5BA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,WAAA,QAAA,CAAA,MAAA,GAAA,KAAA,MAAA;AACA,WAAA,QAAA,CAAA,SAAA,GAAA,KAAA,SAAA;AACA,WAAA,QAAA,CAAA,KAAA,GAAA,KAAA,KAAA;AACA,WAAA,QAAA,CAAA,WAAA,GAAA,KAAA,WAAA;AACA,WAAA,QAAA,CAAA,KAAA,GAAA,KAAA,KAAA;AACA,WAAA,QAAA,CAAA,MAAA,GAAA,YAAA;AACA,WAAA,QAAA,CAAA,EAAA,GAAA,MAAA,CAAA,MAAA,EAAA,CAAA;AAEA,WAAA,MAAA,CAAA,KAAA,CAAA,aAAA,GAAA,KAAA,QAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,iBAAA,EAVA,CAYA;;AACA,WAAA,QAAA,GAAA;AACA,QAAA,EAAA,EAAA,EADA;AAEA,QAAA,SAAA,EAAA,EAFA;AAGA,QAAA,KAAA,EAAA,EAHA;AAIA,QAAA,MAAA,EAAA,EAJA;AAKA,QAAA,WAAA,EAAA,EALA;AAMA,QAAA,KAAA,EAAA,IANA;AAOA,QAAA,MAAA,EAAA;AAPA,OAAA,EASA,KAAA,MAAA,GAAA,KATA;AAUA,KAxBA;AAyBA,IAAA,MAzBA,oBAyBA;AACA,WAAA,MAAA,CAAA,KAAA,CAAA,SAAA,GAAA,KAAA,UAAA;AACA,UAAA,YAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,YAAA,IAAA,IAAA,EAAA;AACA,QAAA,YAAA,CAAA,MAAA,GAAA,KAAA,MAAA;AACA,QAAA,YAAA,CAAA,SAAA,GAAA,KAAA,SAAA;AACA,QAAA,YAAA,CAAA,KAAA,GAAA,KAAA,KAAA;AACA,QAAA,YAAA,CAAA,WAAA,GAAA,KAAA,WAAA;AACA,QAAA,YAAA,CAAA,KAAA,GAAA,KAAA,KAAA;AAEA,aAAA,MAAA,CAAA,KAAA,CAAA,aAAA,GAAA,KAAA,YAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,gBAAA;AACA,aAAA,MAAA,GAAA,KAAA;AACA;AAEA,KAxCA;AAyCA,IAAA,SAzCA,uBAyCA;AACA,MAAA,KAAA,CAAA,iCAAA,KAAA,UAAA,CAAA;AACA,WAAA,MAAA,CAAA,KAAA,CAAA,SAAA,GAAA,KAAA,UAAA;AACA,UAAA,YAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,QAAA;AACA,MAAA,KAAA,CAAA,uCAAA,YAAA,CAAA,EAAA,CAAA;AACA;AA9CA,GA7BA;AA6EA,EAAA,QAAA,EAAA;AACA,IAAA,eADA,6BACA;AACA,aAAA,KAAA,KAAA,GAAA,MAAA,CAAA,KAAA,KAAA,CAAA,CAAA,MAAA,CAAA,cAAA,CAAA,GAAA,EAAA;AACA;AAHA;AA7EA,CAAA","sourcesContent":["<template>\r\n  <div class=\"text-center\">\r\n    <v-dialog v-model=\"dialog\" width=\"500\">\r\n      <template v-slot:activator=\"{ on }\">\r\n        <v-btn\r\n          depressed\r\n          @click=\"mostrarID()\"\r\n          :class=\"accion === 'nuevo' ? 'success' : 'orange lighten-1'\"\r\n          dark\r\n          v-on=\"on\"\r\n        >\r\n          {{ accion == \"nuevo\" ? \"Nuevo proyecto\" : \"Editar\" }}\r\n        </v-btn>\r\n      </template>\r\n      <v-card>\r\n        <v-card-title class=\"headline blue lighten-2\" primary-title>\r\n          {{ accion == \"nuevo\" ? \"Nuevo\" : \"Editar\" }} proyecto\r\n        </v-card-title>\r\n        <v-card-text>\r\n          <!-- Formulario de nuevo proyecto -->\r\n          <v-form class=\"px-3\" ref=\"form\">\r\n            <!-- Titulo -->\r\n            <v-text-field\r\n              v-model=\"nombre\"\r\n              label=\"Título\"\r\n              prepend-icon=\"mdi-folder\"\r\n              :rules=\"[rules.requerido, rules.contador]\"\r\n              counter=\"50\"\r\n            ></v-text-field>\r\n\r\n            <!-- Categoria -->\r\n            <v-select\r\n              :items=\"categorias\"\r\n              v-model=\"categoria\"\r\n              label=\"Categoria\"\r\n              prepend-icon=\"mdi-shape\"\r\n            ></v-select>\r\n\r\n            <!-- Lider-->\r\n            <v-select\r\n              :items=\"lideres\"\r\n              v-model=\"lider\"\r\n              label=\"Líder\"\r\n              prepend-icon=\"mdi-account\"\r\n            ></v-select>\r\n\r\n            <!-- Descripcion -->\r\n            <v-textarea\r\n              v-model=\"descripcion\"\r\n              label=\"Descripción\"\r\n              prepend-icon=\"mdi-pencil\"\r\n              :rules=\"[rules.requerido]\"\r\n            ></v-textarea>\r\n\r\n            <!-- Fecha -->\r\n            <v-menu max-width=\"290\">\r\n              <template v-slot:activator=\"{ on }\">\r\n                <v-text-field\r\n                  :value=\"fechaFormateada\"\r\n                  label=\"Fecha de vencimiento\"\r\n                  prepend-icon=\"mdi-calendar-range\"\r\n                  v-on=\"on\"\r\n                  :rules=\"[rules.requerido]\"\r\n                ></v-text-field>\r\n              </template>\r\n              <v-date-picker\r\n                v-model=\"plazo\"\r\n                :first-day-of-week=\"1\"\r\n                locale=\"es-AR\"\r\n              >\r\n              </v-date-picker>\r\n            </v-menu>\r\n\r\n            <v-btn\r\n              v-if=\"accion === 'nuevo'\"\r\n              @click=\"crear()\"\r\n              depressed\r\n              dark\r\n              class=\"success\"\r\n            >\r\n              Nuevo Proyecto\r\n            </v-btn>\r\n\r\n            <v-btn\r\n              v-else\r\n              @click=\"editar()\"\r\n              dark\r\n              depressed\r\n              class=\"orange lighten-1\"\r\n            >\r\n              Editar proyecto\r\n            </v-btn>\r\n          </v-form>\r\n        </v-card-text>\r\n      </v-card>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { v4 as uuidv4 } from \"uuid\";\r\nimport moment from \"moment\";\r\nmoment.locale(\"es\");\r\n\r\nexport default {\r\n  props: [\"accion\", \"idProyecto\"],\r\n  data() {\r\n    return {\r\n      dialog: false,\r\n      categorias: [\"Fronted\", \"Backend\", \"Funcional\", \"Redes\", \"Devops\"],\r\n      lideres: [\"Lucas Cantoni\", \"Emiliano Graniero\", \"Guido Ponzio\"],\r\n      nombre: \"\",\r\n      descripcion: \"\",\r\n      plazo: null,\r\n      categoria: \"\",\r\n      lider: \"\",\r\n      proyecto: {\r\n        id: \"\",\r\n        categoria: \"\",\r\n        lider: \"\",\r\n        nombre: \"\",\r\n        descripcion: \"\",\r\n        plazo: null,\r\n        estado: \"\",\r\n      },\r\n      //Los requisitos son la forma de filtrar inputs utilizando el prop :rules que viene definido en Vuetify\r\n      //Cada requisito es una funcion lambda\r\n      rules: {\r\n        requerido: (value) => !!value || \"Campo obligatorio\",\r\n        contador: (value) => value.length <= 50 || \"Máximo 50 caracteres\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    crear() {\r\n      this.proyecto.nombre = this.nombre;\r\n      this.proyecto.categoria = this.categoria;\r\n      this.proyecto.lider = this.lider;\r\n      this.proyecto.descripcion = this.descripcion;\r\n      this.proyecto.plazo = this.plazo;\r\n      this.proyecto.estado = \"enProgreso\";\r\n      this.proyecto.id = String(uuidv4());\r\n\r\n      this.$store.state.nuevoProyecto = this.proyecto;\r\n      this.$store.dispatch(\"agregarProyecto\");\r\n\r\n      // Reinicio el objeto proyecto\r\n      this.proyecto = {\r\n        id: \"\",\r\n        categoria: \"\",\r\n        lider: \"\",\r\n        nombre: \"\",\r\n        descripcion: \"\",\r\n        plazo: null,\r\n        estado: \"\",\r\n      },\r\n      this.dialog = false;\r\n    },\r\n    editar() {\r\n      this.$store.state.idBuscado = this.idProyecto;\r\n      let proyectoEdit = this.$store.getters.proyecto;\r\n      if (proyectoEdit != null) {\r\n        proyectoEdit.nombre = this.nombre;\r\n        proyectoEdit.categoria = this.categoria;\r\n        proyectoEdit.lider = this.lider;\r\n        proyectoEdit.descripcion = this.descripcion;\r\n        proyectoEdit.plazo = this.plazo;\r\n\r\n        this.$store.state.nuevoProyecto = this.proyectoEdit;\r\n        this.$store.dispatch(\"editarProyecto\");\r\n        this.dialog = false;\r\n      }\r\n     \r\n    },\r\n    mostrarID(){\r\n      alert(\"ID que llega al componente: \" + this.idProyecto);\r\n      this.$store.state.idBuscado = this.idProyecto;\r\n      let proyectoEdit = this.$store.getters.proyecto;\r\n      alert(\"ID del proyecto traido del store: \" + proyectoEdit.id);\r\n    },\r\n  },\r\n  computed: {\r\n    fechaFormateada() {\r\n      return this.plazo ? moment(this.plazo).format(\"Do MMMM YYYY\") : \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}