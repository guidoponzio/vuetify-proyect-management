A,GAAA,MAAA,CAAA,MAAA,EAAA,CAAA;AAEA,aAAA,MAAA,CAAA,KAAA,CAAA,UAAA,GAAA,KAAA,KAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,cAAA;AACA;;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,MAAA,GAAA,EAAA;AACA,WAAA,GAAA,GAAA,EAAA;AACA,WAAA,KAAA,GAAA;AACA,QAAA,MAAA,EAAA,EADA;AAEA,QAAA,GAAA,EAAA,EAFA;AAGA,QAAA,EAAA,EAAA;AAHA,OAAA;AAKA,KAlBA;AAmBA,IAAA,MAnBA,oBAmBA;AACA,WAAA,MAAA,CAAA,KAAA,CAAA,cAAA,GAAA,KAAA,OAAA;AACA,UAAA,SAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,KAAA;;AACA,UAAA,SAAA,IAAA,IAAA,EAAA;AACA,QAAA,SAAA,CAAA,MAAA,GAAA,KAAA,MAAA;AACA,QAAA,SAAA,CAAA,GAAA,GAAA,KAAA,GAAA;AAEA,aAAA,MAAA,CAAA,KAAA,CAAA,UAAA,GAAA,KAAA,SAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,aAAA;AAEA;;AACA,WAAA,MAAA,GAAA,KAAA;AACA;AA/BA;AApBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"text-center\">\r\n    <v-dialog v-model=\"dialog\" width=\"500\">\r\n      <template v-slot:activator=\"{ on }\">\r\n        <v-btn\r\n          depressed\r\n          :class=\"accion === 'nuevo' ? 'success' : 'orange lighten-1'\"\r\n          dark\r\n          v-on=\"on\"\r\n          >{{ accion == \"nuevo\" ? \"Nuevo integrante\" : \"Editar\" }}\r\n        </v-btn>\r\n      </template>\r\n      <v-card>\r\n        <v-card-title class=\"headline blue lighten-2 white--text\" primary-title>\r\n          {{ accion == \"nuevo\" ? \"Nuevo\" : \"Editar\" }} integrante\r\n        </v-card-title>\r\n        <v-card-text>\r\n          <!-- Formulario de nuevo integrante -->\r\n          <v-form class=\"px-3\" ref=\"form\">\r\n            <!-- Nombre -->\r\n            <v-text-field\r\n              v-model=\"nombre\"\r\n              label=\"Nombre completo\"\r\n              prepend-icon=\"mdi-folder\"\r\n              :rules=\"[rules.requerido, rules.contador]\"\r\n              counter=\"30\"\r\n            ></v-text-field>\r\n\r\n            <!-- Rol -->\r\n            <v-textarea\r\n              v-model=\"rol\"\r\n              label=\"Rol\"\r\n              prepend-icon=\"mdi-pencil\"\r\n              :rules=\"[rules.requerido]\"\r\n            ></v-textarea>\r\n\r\n            <!-- Avatar \r\n            <v-textarea\r\n              v-model=\"avatar\"\r\n              label=\"avatar\"\r\n              prepend-icon=\"mdi-pencil\"\r\n            ></v-textarea>-->\r\n\r\n            <v-btn v-if=\"accion == 'nuevo' \"  @click=\"crear\" depressed dark class=\"success\"\r\n              >Nuevo integrante\r\n            </v-btn>\r\n\r\n            <v-btn v-else @click=\"editar\" depressed dark class=\"orange lighten-1\"\r\n              >Editar integrante\r\n            </v-btn>\r\n          </v-form>\r\n        </v-card-text>\r\n      </v-card>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { v4 as uuidv4 } from \"uuid\";\r\n\r\nexport default {\r\n  props: [\"accion\", \"idLider\"],\r\n  data() {\r\n    return {\r\n      dialog: false,\r\n      nombre: \"\",\r\n      rol: \"\",\r\n      lider:{\r\n        nombre: \"\",\r\n        rol: \"\",\r\n        id: \"\"\r\n      },\r\n      //Los requisitos son la forma de filtrar inputs utilizando el prop :rules que viene definido en Vuetify\r\n      //Cada requisito es una funcion lambda\r\n      rules: {\r\n        requerido: (value) => !!value || \"Campo obligatorio\",\r\n        contador: (value) => value.length <= 30 || \"MÃ¡ximo 30 caracteres\",\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    crear() {\r\n      if (this.$refs.form.validate()) {\r\n        this.lider.nombre = this.nombre;\r\n        this.lider.rol = this.rol;\r\n        this.lider.id = String(uuidv4());\r\n\r\n        this.$store.state.nuevoLider = this.lider;\r\n        this.$store.dispatch('agregarLider');\r\n      }\r\n      this.dialog = false;\r\n      this.nombre = \"\";\r\n      this.rol = \"\";\r\n      this.lider = {\r\n        nombre: \"\",\r\n        rol: \"\",\r\n        id: \"\"\r\n      }\r\n    },\r\n    editar(){\r\n        this.$store.state.idBuscadoLider = this.idLider;\r\n      let liderEdit = this.$store.getters.lider;\r\n      if (liderEdit != null) {\r\n        liderEdit.nombre = this.nombre;\r\n        liderEdit.rol = this.rol;\r\n\r\n        this.$store.state.nuevoLider = this.liderEdit;\r\n        this.$store.dispatch(\"editarLider\");\r\n        \r\n      }\r\n      this.dialog = false;\r\n    }\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}